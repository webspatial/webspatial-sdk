{
  "name": "@webspatial/react-sdk",
  "version": "0.1.6",
  "description": "React components for WebSpatial",
  "main": "./dist/default/index.js",
  "type": "module",
  "types": "./dist/default/index.d.ts",
  "exports": {
    ".": "./dist/default/index.js",
    "./default": "./dist/default/index.js",
    "./web": "./dist/web/index.js",
    "./plugin-esbuild.js": "./npm/plugin/esbuild.js",
    "./plugin-vite.js": "./npm/plugin/vite.js",
    "./plugin-rollup.js": "./npm/plugin/rollup.js",
    "./plugin-webpack.js": "./npm/plugin/webpack.js",
    "./jsx-runtime": {
      "react-server": "./dist/jsx/jsx-runtime.react-server.js",
      "default": "./dist/jsx/jsx-runtime.js"
    },
    "./jsx-dev-runtime": {
      "react-server": "./dist/jsx/jsx-dev-runtime.react-server.js",
      "default": "./dist/jsx/jsx-dev-runtime.js"
    }
  },
  "files": [
    "dist",
    "npm"
  ],
  "scripts": {
    "test": "tsc -p ./tsconfig.json",
    "dev": "tsup --watch",
    "build": "tsup",
    "start": "npm run dev",
    "format": "npx prettier --write 'src/**/*.ts' 'src/**/*.tsx' || true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webspatial/webspatial-sdk.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/webspatial/webspatial-sdk/issues"
  },
  "homepage": "https://github.com/webspatial/webspatial-sdk#readme",
  "dependencies": {
    "@google/model-viewer": "^4.0.0",
    "@types/three": "^0.170.0",
    "lodash.isequal": "^4.5.0",
    "loglevel": "^1.9.1",
    "three": "^0.170.0"
  },
  "peerDependencies": {
    "@webspatial/core-sdk": "workspace:*",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
