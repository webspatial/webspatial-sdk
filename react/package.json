    "@types/lodash.isequal": "^4.5.8",
    "@types/node": "^22.5.5",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/unist": "^3.0.2",
    "esbuild": "^0.23.1",
    "esbuild-plugin-d.ts": "^1.3.1",
    "esbuild-plugin-define": "^0.5.0",
    "tiny-glob": "^0.2.9",
    "tsup": "^8.4.0",
    "typedoc": "^0.26.5",
    "typedoc-plugin-markdown": "^4.2.2",
    "typescript": "^5.5.4",
    "webpack": "^5.98.0"
  }
