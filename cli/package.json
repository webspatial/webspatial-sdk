    "inquirer": "^8.2.4",
    "jimp": "^0.22.7",
    "mime-types": "^2.1.27",
    "minimist": "^1.2.8",
    "node-fetch": "2.6.1",
    "semver": "^7.5.2",
    "sharp": "^0.33.5",
    "valid-url": "^1.0.9",
    "xcode": "^3.0.1"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.2",
    "@types/cli-progress": "^3.7.0",
