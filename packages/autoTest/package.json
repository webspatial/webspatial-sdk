{
  "name": "autotest",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "devAVP": "XR_ENV=avp vite",
    "build": "tsc -b && vite build",
    "build:avp": "vite build && XR_ENV=avp vite build",
    "build-test": "tsc -p tsconfig.json --outDir dist",
    "test": "npm run build-test && mocha --require source-map-support/register --reporter mochawesome --reporter-options reportDir=reports,reportFilename=autotest,overwrite=true,html=true,json=true dist/tests/**/*.test.js",
    "post-test": "ts-node scripts/summarize-results.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/chai": "^5.2.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.1.0",
    "@types/react": "^18.3.21",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.20",
    "chai": "^5.2.0",
    "mocha": "^11.1.0",
    "mochawesome": "^7.1.3",
    "puppeteer": "^24.21.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "vite": "^6.3.1"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.17",
    "@webspatial/core-sdk": "workspace:*",
    "@webspatial/react-sdk": "workspace:*",
    "axios": "^1.13.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^6.25.1",
    "tailwindcss": "^3.4.3"
  }
}
